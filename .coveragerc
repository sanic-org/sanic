[run]
branch = True
source = sanic
omit =
    site-packages
    sanic/__main__.py
    sanic/compat.py
    sanic/reloader_helpers.py
    sanic/simple.py
    sanic/utils.py
    sanic/cli

[html]
directory = coverage

[report]
exclude_lines =
    no cov
    no qa
    noqa
    NOQA
    pragma: no cover
    TYPE_CHECKING
omit =
    site-packages
    sanic/__main__.py
    sanic/compat.py
    sanic/reloader_helpers.py
    sanic/simple.py
    sanic/utils.py
    sanic/cli
skip_empty = True
